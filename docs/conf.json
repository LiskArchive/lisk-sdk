{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": [
			"./docs/typedef/",
			"./app.js",
			"./api/",
			"./config/",
			"./db/",
			"./helpers/",
			"./logic/",
			"./modules/",
			"./schema/"
		],
		"exclude": ["./node_modules"]
	},
	"plugins": ["plugins/markdown"],
	"opts": {
		"readme": "docs/intro.md",
		"encoding": "utf8",
		"destination": "./docs/jsdoc",
		"recurse": true
	},
	"templates": {
		"cleverLinks": false
	}
}
