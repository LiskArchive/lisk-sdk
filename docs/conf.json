{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": [
			"./docs/typedef/",
			"./framework/src/modules/chain/chain.js",
			"./framework/src/modules/chain/api/",
			"./framework/src/modules/chain/config/",
			"./framework/src/components/storage/",
			"./framework/src/controller/",
			"./framework/src/modules/chain/logic/",
			"./framework/src/modules/chain/modules/",
			"./framework/src/modules/chain/schema/"
		],
		"exclude": ["./node_modules"]
	},
	"plugins": ["plugins/markdown"],
	"opts": {
		"readme": "docs/intro.md",
		"encoding": "utf8",
		"destination": "./docs/jsdoc",
		"recurse": true
	},
	"templates": {
		"cleverLinks": false
	}
}
