{
	"name": "lisk-commander",
	"version": "5.0.1-alpha.2",
	"description": "A command line interface for Lisk",
	"author": "Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>",
	"license": "Apache-2.0",
	"keywords": [
		"lisk",
		"blockchain",
		"command-line",
		"nodejs",
		"javascript"
	],
	"homepage": "https://github.com/LiskHQ/lisk-sdk/tree/master/commander#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LiskHQ/lisk-sdk.git"
	},
	"bugs": {
		"url": "https://github.com/LiskHQ/lisk-sdk/issues"
	},
	"engines": {
		"node": ">=12.13.0 <=12",
		"npm": ">=6.12.0"
	},
	"main": "dist/index.js",
	"bin": {
		"lisk": "./bin/run"
	},
	"scripts": {
		"start": "ts-node src/index.js",
		"format": "prettier --write '**/*'",
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"test": "TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=./test/tsconfig.json mocha",
		"test:coverage": "TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=./test/tsconfig.json nyc mocha",
		"test:ci": "TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=./test/tsconfig.json nyc mocha",
		"test:watch": "npm run test:local -- --watch",
		"test:watch:min": "npm run test:watch -- --reporter=min",
		"prebuild": "if test -d dist; then rm -r dist; fi; rm -f tsconfig.tsbuildinfo; rm -f npm-shrinkwrap.json",
		"build": "tsc",
		"prepack": "oclif-dev manifest && npm shrinkwrap && npm prune --production && npm shrinkwrap",
		"prepublishOnly": "rm -r ./node_modules && npm install && npm run lint && npm run build"
	},
	"oclif": {
		"commands": "./dist/commands",
		"bin": "lisk",
		"plugins": [
			"@oclif/plugin-help"
		],
		"topics": {
			"account": {
				"description": "Commands relating to Lisk accounts."
			},
			"help": {
				"description": "Displays help."
			},
			"message": {
				"description": "Commands relating to user messages."
			},
			"passphrase": {
				"description": "Commands relating to Lisk passphrases."
			}
		}
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/dist",
		"/docs"
	],
	"dependencies": {
		"@liskhq/lisk-cryptography": "^3.0.1-alpha.2",
		"@liskhq/lisk-passphrase": "^3.0.1",
		"@liskhq/lisk-validator": "^0.5.1-alpha.2",
		"@oclif/command": "1.5.19",
		"@oclif/config": "1.14.0",
		"@oclif/errors": "1.2.2",
		"@oclif/plugin-help": "2.2.3",
		"bip39": "3.0.2",
		"chalk": "3.0.0",
		"cli-table3": "0.5.1",
		"fs-extra": "8.1.0",
		"inquirer": "7.0.6",
		"listr": "0.14.3",
		"semver": "7.1.3",
		"strip-ansi": "6.0.0",
		"tar": "6.0.1",
		"tslib": "1.11.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "1.22.2",
		"@oclif/test": "1.2.5",
		"@types/bip39": "3.0.0",
		"@types/chai": "4.2.10",
		"@types/chai-as-promised": "7.1.2",
		"@types/expect": "24.3.0",
		"@types/fs-extra": "8.1.0",
		"@types/inquirer": "6.5.0",
		"@types/jquery": "3.3.33",
		"@types/listr": "0.14.2",
		"@types/mocha": "8.0.3",
		"@types/node": "12.12.11",
		"@types/semver": "7.1.0",
		"@types/sinon": "7.5.2",
		"@types/sinon-chai": "3.2.3",
		"@types/strip-ansi": "5.2.1",
		"@types/tar": "4.0.3",
		"@typescript-eslint/eslint-plugin": "3.10.1",
		"@typescript-eslint/parser": "3.10.1",
		"chai": "4.2.0",
		"chai-as-promised": "7.1.1",
		"eslint": "7.8.1",
		"eslint-config-lisk-base": "1.2.2",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jest": "24.0.0",
		"mocha": "8.1.3",
		"nyc": "15.0.0",
		"prettier": "2.0.5",
		"rxjs-compat": "6.5.4",
		"sinon": "9.0.0",
		"sinon-chai": "3.5.0",
		"source-map-support": "0.5.19",
		"ts-node": "8.6.2",
		"tsconfig-paths": "3.9.0",
		"typescript": "3.8.3"
	}
}
