{
	"name": "pos-sidechain-example-two",
	"version": "0.1.0",
	"description": "Lisk-SDK Application",
	"author": "Lisk Foundation &amp;lt;admin@lisk.com&amp;gt;, lightcurve GmbH &amp;lt;admin@lightcurve.io&amp;gt;",
	"license": "Apache-2.0",
	"keywords": [
		"blockchain",
		"lisk",
		"nodejs",
		"javascript",
		"typescript"
	],
	"homepage": "",
	"repository": {},
	"engines": {
		"node": ">=16.14.1 <=16",
		"npm": ">=8.1.0"
	},
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint --ext .ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"format": "prettier --write '**/*'",
		"prepack": "oclif manifest && oclif readme --multi --dir=docs/commands && npm shrinkwrap && npm prune --production && npm shrinkwrap",
		"prebuild": "if test -d dist; then rm -r dist; fi; rm -f tsconfig.tsbuildinfo; rm -f npm-shrinkwrap.json",
		"start": "echo Run \"./bin/run start\" to start the app",
		"build": "tsc",
		"test": "jest --passWithNoTests",
		"test:coverage": "jest --passWithNoTests --coverage=true --coverage-reporters=text",
		"test:ci": "jest --passWithNoTests --coverage=true --coverage-reporters=json",
		"version": "oclif readme --multi --dir=docs/commands && git add README.md docs",
		"prepublishOnly": "npm ci && npm run lint && npm run build",
		"app": "./bin/run start --api-ipc --api-http --log debug --enable-chain-connector-plugin"
	},
	"bin": {
		"pos-sidechain-example-two": "./bin/run"
	},
	"lisk": {
		"addressPrefix": "lsk"
	},
	"oclif": {
		"bin": "pos-sidechain-example-two",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-version"
		],
		"additionalHelpFlags": [
			"-h"
		],
		"additionalVersionFlags": [
			"-v"
		],
		"topicSeparator": " ",
		"topics": {
			"block": {
				"description": "Commands relating to pos-sidechain-example-two blocks."
			},
			"blockchain": {
				"description": "Commands relating to pos-sidechain-example-two blockchain data."
			},
			"console": {
				"description": "pos-sidechain-example-two interactive REPL session to run commands."
			},
			"config": {
				"description": "Commands relating to pos-sidechain-example-two node configuration."
			},
			"keys": {
				"description": "Commands relating to pos-sidechain-example-two key generation."
			},
			"generator": {
				"description": "Commands relating to pos-sidechain-example-two block generator."
			},
			"hash-onion": {
				"description": "Create hash onions to be used by the forger."
			},
			"system": {
				"description": "Commands relating to pos-sidechain-example-two node."
			},
			"passphrase": {
				"description": "Commands relating to pos-sidechain-example-two passphrases."
			},
			"endpoint": {
				"description": "Commands relating to pos-sidechain-example-two endpoint."
			},
			"sdk": {
				"description": "Commands relating to Lisk SDK development."
			},
			"transaction": {
				"description": "Commands relating to pos-sidechain-example-two transactions."
			}
		}
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/dist",
		"/config",
		"/docs"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@liskhq/lisk-framework-dashboard-plugin": "0.2.0-alpha.22",
		"@liskhq/lisk-framework-faucet-plugin": "0.2.0-alpha.22",
		"@liskhq/lisk-framework-monitor-plugin": "0.3.0-alpha.22",
		"@liskhq/lisk-framework-forger-plugin": "0.3.0-alpha.22",
		"@liskhq/lisk-framework-report-misbehavior-plugin": "0.3.0-alpha.22",
		"@liskhq/lisk-framework-chain-connector-plugin": "0.1.0-alpha.11",
		"@oclif/core": "1.20.4",
		"@oclif/plugin-autocomplete": "1.3.6",
		"@oclif/plugin-help": "5.1.19",
		"@oclif/plugin-version": "1.1.3",
		"axios": "0.21.2",
		"fs-extra": "11.1.0",
		"inquirer": "8.2.5",
		"lisk-commander": "6.0.0-alpha.22",
		"lisk-sdk": "6.0.0-alpha.22",
		"tar": "6.1.11",
		"tslib": "2.4.1"
	},
	"devDependencies": {
		"@oclif/test": "2.2.12",
		"@types/fs-extra": "9.0.13",
		"@types/node": "16.18.3",
		"@types/tar": "6.1.3",
		"@types/jest": "29.2.3",
		"@types/jest-when": "3.5.2",
		"@typescript-eslint/eslint-plugin": "5.44.0",
		"@typescript-eslint/parser": "5.44.0",
		"eslint": "8.28.0",
		"eslint-config-lisk-base": "2.0.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jest": "27.1.6",
		"globby": "10.0.2",
		"husky": "4.2.5",
		"jest": "29.3.1",
		"jest-extended": "3.2.0",
		"jest-when": "3.5.2",
		"oclif": "3.2.31",
		"lint-staged": "10.2.11",
		"prettier": "2.8.0",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"typescript": "5.0.2"
	}
}
