		"style-loader": "1.3.0",
		"terser-webpack-plugin": "4.2.3",
		"ts-jest": "26.3.0",
		"ts-node": "8.6.2",
		"ts-pnp": "1.2.0",
		"tsconfig-paths": "3.9.0",
		"typescript": "3.8.3",
