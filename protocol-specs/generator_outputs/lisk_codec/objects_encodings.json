{
	"title": "Encondings for objects types supported by lisk-codec",
	"summary": "Examples of encoding objects with lisk-codec",
	"config": {
		"network": "devnet"
	},
	"runner": "lisk_codec",
	"handler": "objects_encodings",
	"testCases": [
		{
			"description": "Encoding of object",
			"input": {
				"object": {
					"address": {
						"type": "Buffer",
						"data": [
							225,
							26,
							17,
							54,
							71,
							56,
							34,
							88,
							19,
							248,
							110,
							168,
							82,
							20,
							64,
							14,
							93,
							176,
							141,
							110
						]
					},
					"balance": 10000000,
					"isDelegate": true,
					"name": "delegate",
					"asset": {
						"data": "Check out the Lisk SDK now in binary!"
					}
				},
				"schema": {
					"$id": "object11",
					"type": "object",
					"properties": {
						"address": {
							"dataType": "bytes",
							"fieldNumber": 1
						},
						"balance": {
							"dataType": "uint64",
							"fieldNumber": 2
						},
						"isDelegate": {
							"dataType": "boolean",
							"fieldNumber": 3
						},
						"name": {
							"dataType": "string",
							"fieldNumber": 4
						},
						"asset": {
							"type": "object",
							"fieldNumber": 5,
							"properties": {
								"data": {
									"dataType": "string",
									"fieldNumber": 1
								}
							}
						}
					}
				}
			},
			"output": {
				"value": "0a14e11a11364738225813f86ea85214400e5db08d6e1080ade2041801220864656c65676174652a270a25436865636b206f757420746865204c69736b2053444b206e6f7720696e2062696e61727921"
			}
		},
		{
			"description": "Encoding of object with optional property",
			"input": {
				"object": {
					"isActive": true,
					"value": 1
				},
				"schema": {
					"$id": "object12",
					"type": "object",
					"properties": {
						"isActive": {
							"dataType": "boolean",
							"fieldNumber": 1
						},
						"data": {
							"dataType": "bytes",
							"fieldNumber": 2
						},
						"value": {
							"dataType": "uint64",
							"fieldNumber": 3
						}
					}
				}
			},
			"output": {
				"value": "08011801"
			}
		}
	]
}
